{
  "name": "instagram-cropper",
  "version": "1.0.0",
  "description": "Instagram image cropper and resizer tool",
  "main": "index.html",
  "scripts": {
    "build-css": "tailwindcss -i ./src/input.css -o ./assets/css/style.css --watch",
    "build-css-prod": "tailwindcss -i ./src/input.css -o ./assets/css/style.css --minify",
    "dev": "npm run build-css",
    "build": "npm run build-css-prod"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0"
  },
  "keywords": [
    "instagram",
    "image-cropper",
    "tailwind",
    "responsive"
  ],
  "author": "Reduce Image Size",
  "license": "MIT"
}
